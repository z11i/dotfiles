{
  "cursor.cmdk.useThemedDiffBackground": true,
  "cursor.cpp.enablePartialAccepts": true,
  "cursor.diffs.useCharacterLevelDiffs": true,
  "cursor.terminal.usePreviewBox": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.fontFamily": "'Berkeley Mono SemiCondensed'",
  "editor.inlayHints.padding": true,
  "editor.language.colorizedBracketPairs": [],
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "deepIndent",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "gitlens.currentLine.scrollable": false,
  "go.inlayHints.constantValues": true,
  "go.toolsManagement.autoUpdate": true,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.lineHeight": 1.15,
  // "vim.enableNeovim": true,
  // "vim.neovimPath": "/Users/z11i/.local/bin/nvim",
  // "vim.neovimUseConfigFile": true,
  // "vim.flash.enable": true,
  "window.autoDetectColorScheme": true,
  "workbench.colorTheme": "Tokyo Night Light",
  "workbench.editor.pinnedTabsOnSeparateRow": true,
  "gitlens.hovers.changesDiff": "hunk",
  "git.openRepositoryInParentFolders": "always",
  "terminal.integrated.copyOnSelection": true,
  "markdown.preview.typographer": true,
  // "ruff.nativeServer": "on",
  "terminal.integrated.fontLigatures": true,
  "editor.fontSize": 14.4,
  "terminal.integrated.fontFamily": "'Berkeley Mono SemiCondensed'",
  "editor.codeActionsOnSave": {},
  "terminal.integrated.macOptionIsMeta": true,
  "terminal.integrated.scrollback": 5000,
  "csv.enabled": true,
  "terminal.external.osxExec": "WezTerm.app",
  "terminal.integrated.confirmOnExit": "hasChildProcesses",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "underline",
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.suggest.enabled": true,
  "terminal.integrated.stickyScroll.enabled": true,
  "workbench.preferredLightColorTheme": "Solarized Light",
  "indentRainbow.includedLanguages": ["py", "yaml"],
  "indentRainbow.indicatorStyle": "light",
  "workbench.editor.pinnedTabSizing": "shrink",
  "editor.accessibilitySupport": "off",
  "cursor.composer.collapsePaneInputBoxPills": true
